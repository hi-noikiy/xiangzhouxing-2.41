/* pages/car-code/code-detail/index.wxss */
.container {
  width: 100%;
  background: #f5f5f5;
  box-sizing: border-box;
}

.header{
  position: relative;
  height: 680rpx;
  background: #fff;
  box-sizing: border-box;
}

.header-field {
  padding: 50rpx 95rpx;
}

.cro-left-bottom,
.cro-right-bottom,
.cro-left-top,
.cro-right-top {
  position: absolute;
  width: 20rpx;
  height: 20rpx;
  z-index: 1;
  background: #f5f5f5;
}

.cro-line-bottom {
  position: absolute;
  bottom: 1px;
  left: 100rpx;
  width: 480rpx;
  border-bottom: 2px dashed #f5f5f5;
}

.cro-left-bottom {
  bottom: 0;
  left: 0;
  border-radius:0 20rpx 0 0;
}

.cro-right-bottom {
  bottom: 0;
  right: 0;
  border-radius: 20rpx 0 0 0;
}

.cro-left-top {
  top: 0;
  left: 0;
  border-radius:0 0 20rpx 0;
}

.cro-right-top {
  top: 0;
  right: 0;
  border-radius: 0 0 0 20rpx;
}

.header-field__code {
  width: 480rpx;
  height: 480rpx;
  margin: 0 auto;
}

.content {
  position: relative;
  background: #fff;
  padding-top: 36rpx;
}

.line {
  height: 15rpx;
  width: 100%;
  background-color: rgb(230, 230, 230);
}

.edit-text{
  position: absolute;
  right: 40rpx;
  top: 70rpx;
  background: #4293F4;
  border: 1px solid rgba(0,0,0,0.10);
  border-radius: 8rpx;
  color: #fff;
  width: 146rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
}
