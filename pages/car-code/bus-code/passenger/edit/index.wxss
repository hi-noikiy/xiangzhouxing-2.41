/* pages/car-code/bus-code/passenger/edit/index.wxss */
.sk-dropdown-list {
  display: inline-block;
  width: 100%;
  padding: 15px 20px;
  box-sizing: border-box;
}
.sk-dropdown-list__head {
  display: flex;
}
.sk-dropdown-list__title {
  flex: 1;
  font-size: 48rpx;
  line-height: 68rpx;
  font-weight: bold;
  word-break: break-all;
}
.sk-dropdown-list__number {
  font-size: 16px;
  margin-right: 4px;
}
.sk-dropdown-list__content {
  position: relative;
}
.sk-dropdown-list__content.is-hidden {
  height: 0;
  overflow: hidden;
}
.sk-dropdown-list__btn {
  display: flex;
  align-items: center;
  width: 290rpx;
  height: 54rpx;
  line-height: 32rpx;
  background: none;
  border: 0;
  font-size: 12px;
  margin: 8px 4px 4px 0;
  padding: 4px 0;
}
.sk-dropdown-list__plus {
  width: 44rpx;
  height: 44rpx;
  margin: 0 22rpx 0 30rpx;
}
.sk-dropdown-list__plus-text {
  opacity: 0.9;
  font-family: PingFangSC-Regular;
  font-size: 36rpx;
  color: #4293F4;
  letter-spacing: 0;
  line-height: 48rpx;
}
.sk-dropdown-list__ul {
  padding: 8rpx 0 8rpx 30rpx;
}
.sk-dropdown-list__item {
  display: flex;
  align-items: center;
}
.sk-dropdown-list__delete {
  width: 36rpx;
  height: 36rpx;
  padding: 30rpx 18rpx;
}
.sk-dropdown-list__box {
  flex: 1;
  display: flex;
  height: 100rpx;
  padding: 30rpx 45rpx;
  border: 1px solid #EBEBEB;
  margin: 10rpx 0;
  border-radius: 8rpx;
  box-sizing: border-box;
}
.sk-dropdown-list__item .s-name {
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #000000;
  letter-spacing: 0;
  line-height: 40rpx;
  width: 210rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
.sk-dropdown-list__item .s-phone {
  flex: 1;
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #999999;
  letter-spacing: 0;
  text-align: right;
  line-height: 40rpx;
}

.sk-pop-layer {
  padding: 36rpx 40rpx;
  box-sizing: border-box;
}
.sk-pop-layer__title {
  display: block;
  font-family: PingFangSC-Medium;
  font-weight: 300;
  font-size: 36rpx;
  color: #000000;
  letter-spacing: 0;
  line-height: 48rpx;
}
.sk-pop-layer__descript {
  display: block;
  padding: 6rpx 0;
  font-family: PingFangSC-Regular;
  font-weight: 300;
  font-size: 30rpx;
  color: #999999;
  letter-spacing: 0;
  line-height: 40rpx;
}
.sk-pop-layer__scan {
  height: 180rpx;
  margin: 20rpx 0;
  padding: 30rpx 0;
  background: #FFFFFF;
  border: 1px solid #EBEBEB;
  box-sizing: border-box;
}
.sk-pop-layer__scan .s-icon {
  display: block;
  width: 86rpx;
  height: 86rpx;
  margin: 0 auto;
}
.sk-pop-layer__scan .s-text {
  display: block;
  font-weight: 300;
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  padding-top: 12rpx;
  color: #999999;
  letter-spacing: 0;
  text-align: center;
  line-height: 40rpx;
}

.sk-recommer-recently__title {
  display: block;
  font-family: PingFangSC-Medium;
  font-size: 36rpx;
  color: #000000;
  letter-spacing: 0;
  line-height: 48rpx;
  font-weight: 400;
}

.sk-recommer-recently__empty {
  display: block;
  font-family: PingFangSC-Regular;
  padding: 40rpx 0;
  font-size: 28rpx;
  color: #999999;
  letter-spacing: 0;
  line-height: 48rpx;
  text-align: center;
}

.submit-btn{
    width: 90%;
}
.checkbox_group{
  /* border:1px solid red; */
}
.checkbox_group checkbox-group{
  overflow: auto;
  max-height: 508rpx;
}

.sk-scan-list{

}
.sk-scan-list_title{
  display: block;
  /* padding: 6rpx 0; */
  font-family: PingFangSC-Regular;
  font-weight: 300;
  font-size: 35rpx;
  color: #999999;
  letter-spacing: 0;
  line-height: 40rpx;
  padding: 15rpx 0;
}
.sk-scan-list_container{
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  padding-bottom: 30rpx;
}
.sk-scan-list_container .sk-scan-list_item{
  font-family: PingFangSC-Regular;
  font-size: 35rpx;
  color: #000000;
  letter-spacing: 0;
  line-height: 40rpx;
  padding: 10rpx 0;
}